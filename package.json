{
  "name": "neofelis",
  "private": true,
  "version": "2.0.0",
  "description": "Neofelis",
  "scripts": {
    "init": "pnpm node scripts/generate-keys.ts && pnpm install",
    "prepare": "husky install",
    "DEV": "---------------------------------DEV--------------------------------",
    "dev": "concurrently \"next dev \" \"pnpm run generate-graphql \"",
    "generate-graphql": "graphql-codegen --watch --config codegen.yml --require dotenv/config",
    "build": "pnpm run docs:build && next build",
    "start": "next start",
    "SB": "---------------------------------STORY-BOOK--------------------------",
    "storybook": "set NODE_OPTIONS=--openssl-legacy-provider && start-storybook -p 6006 -s ./public",
    "storybook:build": "build-storybook -s ./public && mv ./storybook-static ./public/storybook",
    "STATIC": "---------------------------------STATIC--------------------------",
    "static:build": "pnpm run build && next export",
    "static:serve": "pnpm run static:build && pnpmx run servor out app.html 5000",
    "docs:build": "swagger-cli bundle src/mocks/openapi/openapi.yaml -t json --outfile public/open-api.json",
    "CODE": "---------------------------------UTILS-----------------------------",
    "pretty": "prettier -w .",
    "lint": "next lint",
    "test": "jest --roots src --watch",
    "test:ci": "jest --roots src",
    "type-check": "tsc",
    "RELEASE": "-----------------------------RELEASE---------------------------",
    "release": "release-it"
  },
